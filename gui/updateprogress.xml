<?xml version="1.0" encoding="UTF-8"?>
<GUI virtualw="1000" virtualh="1000" font="fonts/vtcorona.ttf" visible="false">
   <Label x="0" y="0" width="1000" height="1000" normal="textures/menubg.png"/>
   <Label x="400" y="650" width="200" height="30" align="center" text="Updating" name="updatename"
          normal="textures/buttonnorm.png"/>
   <Label x="250" y="700" width="500" height="190" name="updateprogress"
               normal="textures/buttonnorm.png">
      <Label x="20" y="10" width="460" height="30" text="Update Progress..." name="progresstext"
              normal="textures/buttonnorm.png"/>
      <Label x="20" y="50" width="460" height="30" name="fileprogresstext"
              normal="textures/buttonnorm.png"/>
      <ProgressBar x="20" y="100" width="460" height="30" name="fileprogressbar"
                   normal="textures/buttonhover.png"
                   hover="textures/buttonpressed.png"/>
      <ProgressBar x="20" y="140" width="460" height="30" name="updateprogressbar"
                   normal="textures/buttonhover.png"
                   hover="textures/buttonpressed.png"/>
   </Label>
   <Button x="400" y="900" width="200" height="60" text="Cancel" leftclick="cancelupdate" align="center"
           normal="textures/buttonnorm.png"
           hover="textures/buttonhover.png"
           pressed="textures/buttonpressed.png"/>
   <Label x="0" y="0" width="1000" height="1000" normal="textures/menuoverlay.png"/>
</GUI>