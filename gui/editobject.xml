<?xml version="1.0" encoding="UTF-8"?>
<GUI virtualw="1000" virtualh="1000" font="fonts/jackinput.ttf" visible="false"
     buttonnormal="textures/hud/button.png"
     buttonpressed="textures/hud/button.png"
     buttonhover="textures/hud/button.png"
     slidernormal="textures/sliderbuttonnorm.png"
     sliderpressed="textures/sliderbuttonnorm.png"
     sliderhover="textures/sliderbuttonnorm.png"
     bgnormal="textures/hud/button.png"
     bgpressed="textures/hud/button.png"
     bghover="textures/hud/button.png"
     cursornormal="textures/sliderbuttonnorm.png">
   <ScrollView x="5" y="5" width="500" height="800" name="Base"
               normal="textures/hud/button.png">
      <Label x="10" y="10" width="150" height="30" text="Name">
         <LineEdit x="150" y="0" width="300" height="30" name="ObjectName"/>
      </Label>
      <Label x="10" y="50" width="150" height="30" text="Position">
         <Label x="0" y="30" width="150" height="30" name="ObjectX"/>
         <Label x="150" y="30" width="150" height="30" name="ObjectY"/>
         <Label x="300" y="30" width="150" height="30" name="ObjectZ"/>
      </Label>
      
      <Label x="10" y="130" width="150" height="30" text="Rotation">
         <Label x="0" y="30" width="150" height="30" name="ObjectRotX"/>
         <Label x="150" y="30" width="150" height="30" name="ObjectRotY"/>
         <Label x="300" y="30" width="150" height="30" name="ObjectRotZ"/>
      </Label>
      
      <Label x="10" y="210" width="150" height="30" text="Scale">
         <LineEdit x="150" y="0" width="150" height="30" name="ObjectScale"/>
      </Label>
      
      <Label x="10" y="600" width="150" height="30" text="File">
         <LineEdit x="0" y="30" width="450" height="30" name="ObjectFile"/>
      </Label>
      
      <Button x="10" y="700" width="100" height="30" text="SaveObject"
              leftclick="saveobject"/>
   </ScrollView>
</GUI>