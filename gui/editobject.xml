<?xml version="1.0" encoding="UTF-8"?>
<GUI virtualw="1000" virtualh="1000" font="fonts/jackinput.ttf" visible="false"
     buttonnormal="textures/hud/button.png"
     buttonpressed="textures/hud/buttonred.png"
     buttonhover="textures/hud/buttonhover.png"
     slidernormal="textures/sliderbuttonnorm.png"
     sliderpressed="textures/sliderbuttonnorm.png"
     sliderhover="textures/sliderbuttonnorm.png"
     bgnormal="textures/hud/button.png"
     bgpressed="textures/hud/button.png"
     bghover="textures/hud/button.png"
     cursornormal="textures/sliderbuttonnorm.png">
   <TabWidget x="5" y="5" width="500" height="800" name="Base"
               normal="textures/hud/button.png">
      <Tab text="Main" buttonwidth="100">
         <Label x="10" y="10" width="150" height="30" text="Name">
            <LineEdit x="150" y="0" width="300" height="30" name="ObjectName"/>
         </Label>
         <Label x="10" y="50" width="150" height="30" text="Position">
            <Label x="0" y="30" width="150" height="30" name="ObjectX"/>
            <Label x="150" y="30" width="150" height="30" name="ObjectY"/>
            <Label x="300" y="30" width="150" height="30" name="ObjectZ"/>
         </Label>
         
         <Label x="10" y="130" width="150" height="30" text="Rotation">
            <Label x="0" y="30" width="150" height="30" name="ObjectRotX"/>
            <Label x="150" y="30" width="150" height="30" name="ObjectRotY"/>
            <Label x="300" y="30" width="150" height="30" name="ObjectRotZ"/>
         </Label>
         
         <Label x="10" y="210" width="150" height="30" text="Scale">
            <LineEdit x="150" y="0" width="150" height="30" name="ObjectScale"/>
         </Label>
         
         <Label x="10" y="600" width="150" height="30" text="File">
            <LineEdit x="0" y="30" width="450" height="30" name="ObjectFile"/>
         </Label>
         
         <Button x="10" y="700" width="100" height="30" text="SaveObject"
               leftclick="saveobject"/>
      </Tab>
      <Tab text="Tree" buttonwidth="100">
         <Label x="10" y="10" width="150" height="30" text="Seed">
            <LineEdit x="150" y="0" width="150" height="30" name="Seed"/>
         </Label>
         <Label x="10" y="40" width="150" height="30" text="Impostor Dist.">
            <LineEdit x="150" y="0" width="150" height="30" name="ImpostorDistance"/>
         </Label>
         <Label x="10" y="70" width="450" height="30" text="Trunk" align="center"
                normal="textures/sliderbuttonnorm.png">
            <Label x="0" y="30" width="150" height="30" text="Trunk Height">
               <LineEdit x="150" y="0" width="150" height="30" name="TrunkHeight"/>
            </Label>
            <Label x="0" y="60" width="150" height="30" text="Trunk Radius">
               <LineEdit x="150" y="0" width="150" height="30" name="TrunkRadius"/>
            </Label>
            <Label x="0" y="90" width="150" height="30" text="Trunk Segments">
               <LineEdit x="150" y="0" width="150" height="30" name="TrunkSegments"/>
            </Label>
            <Label x="0" y="120" width="150" height="30" text="Trunk Slices">
               <LineEdit x="150" y="0" width="150" height="30" name="TrunkSlices"/>
            </Label>
            <Label x="0" y="150" width="150" height="30" text="Trunk Taper">
               <LineEdit x="150" y="0" width="150" height="30" name="TrunkTaper"/>
            </Label>
         </Label>
         
         <Label x="10" y="250" width="450" height="30" text="Branches" align="center"
                normal="textures/sliderbuttonnorm.png">
            <Label x="0" y="30" width="150" height="30" text="Max Angle">
               <LineEdit x="150" y="0" width="150" height="30" name="MaxBranchAngle"/>
            </Label>
            <Label x="0" y="60" width="150" height="30" text="Initial Height">
               <LineEdit x="150" y="0" width="150" height="30" name="InitialHeight"/>
            </Label>
            <Label x="0" y="90" width="150" height="30" text="Height Reduction">
               <LineEdit x="150" y="0" width="150" height="30" name="HeightReduction"/>
            </Label>
            <Label x="0" y="120" width="150" height="30" text="Initial Radius">
               <LineEdit x="150" y="0" width="150" height="30" name="InitialRadius"/>
            </Label>
            <Label x="0" y="150" width="150" height="30" text="Radius Taper">
               <LineEdit x="150" y="0" width="150" height="30" name="RadiusTaper"/>
            </Label>
            <Label x="0" y="180" width="150" height="30" text="Num Levels">
               <LineEdit x="150" y="0" width="150" height="30" name="NumLevels"/>
            </Label>
            <Label x="0" y="210" width="150" height="30" text="Num Slices">
               <LineEdit x="150" y="0" width="150" height="30" name="NumSlices"/>
            </Label>
            <Label x="0" y="240" width="150" height="30" text="Num Segments">
               <LineEdit x="150" y="0" width="150" height="30" name="NumSegments"/>
            </Label>
            <Label x="0" y="270" width="150" height="30" text="Num Branches 0">
               <LineEdit x="150" y="0" width="150" height="30" name="NumBranches0"/>
            </Label>
            <Label x="0" y="300" width="150" height="30" text="Num Branches 1">
               <LineEdit x="150" y="0" width="150" height="30" name="NumBranches1"/>
            </Label>
            <Label x="0" y="330" width="150" height="30" text="Curve Coeff.">
               <LineEdit x="150" y="0" width="150" height="30" name="CurveCoeff"/>
            </Label>
         </Label>
         
         <Label x="10" y="610" width="450" height="30" text="Leaves" align="center"
                normal="textures/sliderbuttonnorm.png">
            <Label x="0" y="30" width="150" height="30" text="Num Leaves">
               <LineEdit x="150" y="0" width="150" height="30" name="NumLeaves"/>
            </Label>
            <Label x="0" y="60" width="150" height="30" text="Leaf Size">
               <LineEdit x="150" y="0" width="150" height="30" name="LeafSize"/>
            </Label>
            <Label x="0" y="90" width="150" height="30" text="Leaf Segments">
               <LineEdit x="150" y="0" width="150" height="30" name="LeafSegs"/>
            </Label>
            <Label x="0" y="120" width="150" height="30" text="Leaf Curve">
               <LineEdit x="150" y="0" width="150" height="30" name="LeafCurve"/>
            </Label>
            <Label x="0" y="150" width="150" height="30" text="First Leaf Level">
               <LineEdit x="150" y="0" width="150" height="30" name="FirstLeafLevel"/>
            </Label>
         </Label>
      </Tab>
   </TabWidget>
</GUI>