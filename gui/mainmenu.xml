<?xml version="1.0" encoding="UTF-8"?>
<GUI virtualw="1000" virtualh="1000" font="fonts/vtcorona.ttf"
     buttonnormal="textures/buttonnorm.png"
     buttonpressed="textures/buttonpressed.png"
     buttonhover="textures/buttonhover.png"
     gutternormal="textures/buttonnorm.png"
     gutterpressed="textures/buttonnorm.png"
     gutterhover="textures/buttonnorm.png"
     slidernormal="textures/sliderbuttonnorm.png"
     sliderpressed="textures/sliderbuttonnorm.png"
     sliderhover="textures/sliderbuttonnorm.png"
     bgnormal="textures/buttonnormsquare.png"
     bgpressed="textures/buttonnormsquare.png"
     bghover="textures/buttonnormsquare.png"
     tablecellnormal="textures/transparent.png"
     tablecellpressed="textures/transparent.png"
     tablecellhover="textures/transparent.png"
     tablerownormal=""
     tablerowpressed="textures/buttonnormsquare.png"
     tablerowhover=""
     cursornormal="textures/sliderbuttonnorm.png"
     leftclicksound="sounds/click.ogg">
   <Label x="0" y="0" width="1000" height="1000" normal="textures/menubg.png"/>
   
   <Label x="300" y="25" width="400" height="200" text="" name="logo" xmargin="0" ymargin="0" align="center"
          normal="textures/logo.png"/>
      <Layout x="390" y="240" padding="10" orientation="vertical">
       <Button width="200" height="40" text="Resume" name="resumebutton" xmargin="3" ymargin="0" align="center" textcolor="fffc"
            leftclick="resume"/>
      </Layout>
    <Layout x="285" y="290" padding="10" orientation="horizontal">
      <Button width="200" height="40" text="Join" name="joinbutton" xmargin="3" ymargin="0" align="center" textcolor="fffc"
            leftclick="join"/>
      <Button width="200" height="40" text="Host" name="hostbutton" xmargin="3" ymargin="0" align="center" textcolor="fffc"
            leftclick="hostsetup"/>
      </Layout>
      <Layout x="390" y="400" padding="10" orientation="vertical">
       <Button width="200" height="40" text="Settings" name="settingsbutton" xmargin="3" ymargin="0" align="center" textcolor="fffc"
            leftclick="showsettings"/>
        <Button width="200" height="40" text="Credits" name="settingsbutton" xmargin="3" ymargin="0" align="center" textcolor="fffc"
            leftclick="showcredits"/>
	</Layout>
	<Layout x="390" y="600" padding="10" orientation="vertical">
        <Button width="200" height="40" text="Exit" name="exitbutton" xmargin="3" ymargin="0" align="center" textcolor="fffc"
            leftclick="exit"/>
        </Layout>
   
   <Label x="0" y="0" width="1000" height="1000" normal="textures/menuoverlay.png"/>
   
</GUI>
