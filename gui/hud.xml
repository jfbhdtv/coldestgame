<?xml version="1.0" encoding="UTF-8"?>
<GUI virtualw="1000" virtualh="1000" font="fonts/jackinput.ttf" visible="false"
     slidernormal="textures/sliderbuttonnorm.png"
     sliderpressed="textures/sliderbuttonnorm.png"
     sliderhover="textures/sliderbuttonnorm.png">

  <!-- Cockpit for First Person View -->
  <Label x="0" y="0" width="1000" height="1000" text="" name="cockpit">
    <Label x="0" y="0" width="1000" height="1000" text="" name="cage"
           normal="textures/hud/cockpit.png"/>
    <Label x="0" y="2" width="225" height="155" text="" name="tmonitor1"
           normal="textures/hud/monitor.png"/>
    <Label x="775" y="2" width="225" height="155" text="" name="tmonitor3"
           normal="textures/hud/monitor.png"/>
    <Label x="230" y="2" width="540" height="155" text="" name="tmonitorcenter"
           normal="textures/hud/monitor.png"/>

    <Label x="0" y="780" width="230" height="215" text="" name="bmonitor1"
           normal="textures/hud/monitor.png"/>
    <Label x="394" y="678" width="212" height="325" text="" name="bmonitor2"
           normal="textures/hud/monitor.png"/>
    <Label x="795" y="780" width="200" height="215" text="" name="bmonitor3"
            normal="textures/hud/monitor.png"/>
  </Label>

  <!-- Status -->
  <Label x="15" y="800" width="75" height="30" text="Heat" name="templabel"/>
  <ProgressBar x="95" y="800" width="100" height="30" name="temperature"
               normal="textures/hud/tempgauge.png"
               hover="textures/hud/buttonred.png"/>
   <Label x="15" y="850" width="150" height="30" text="" name="torsohp"/>
   <Label x="15" y="880" width="150" height="30" text="" name="legshp"/>
   <Label x="15" y="910" width="150" height="30" text="" name="leftarmhp"/>
   <Label x="15" y="940" width="150" height="30" text="" name="rightarmhp"/>



   <!-- The selection indicator labels need to be first so they don't get rendered over the text labels -->
   <!-- Weapons -->
   <Label x="820" y="800" width="150" height="30" text="Weapons" name="weaponslabel"/>

   <Label x="820" y="840" width="150" height="30" text="Torso" name="torsoweapon"/>
   <Label x="820" y="840" width="150" height="30" text="" name="torsoselected"
          normal="textures/hud/button.png"/>
   <Label x="820" y="870" width="150" height="30" text="Left Arm" name="larmweapon"/>
   <Label x="820" y="870" width="150" height="30" text="" name="larmselected"
          normal="textures/hud/button.png"/>
   <Label x="820" y="900" width="150" height="30" text="Right Arm" name="rarmweapon"/>
   <Label x="820" y="900" width="150" height="30" text="" name="rarmselected"
          normal="textures/hud/button.png"/>





   <!-- Center -->
   <Label x="485" y="175" width="30" height="30" name="facingcenter"
          normal="textures/hud/buttonred.png"/>
   <Slider x="400" y="175" width="200" height="30" name="facing" min="-120" max="120"
                normal="textures/hud/button.png"/>
   <!-- targeting -->
   <Label x="490" y="484" width="20" height="32" text="" name="reticle"
          normal="textures/reticle.png"/>
   <Label x="250" y="520" width="500" height="30" text="" name="targetplayer" align="center"/>

   <Table x="10" y="13" width="205" height="135" name="servermessages" columns="1" colwidths="300" rowheight="20"/>

   <!-- Map -->
   <Label x="404" y="697" width="194" height="288" text="" name="minimapback"
          normal="textures/black.png"/>
   <Label x="404" y="697" width="194" height="288" text="" name="minimap"/>
   <Label x="404" y="697" width="194" height="288" text="" name="minimapoverlay"/>

   <!-- Hit indicator-->
   <Label x="0" y="0" width="1000" height="1000" text="" name="hitind"
          normal="textures/hit.png"/>
</GUI>
